{
    "dns": {
        "countryInitials": "US",
        "domain": "fibonaccitest.com",
        "default": "localhost",
        "defaultIP": "127.0.0.1",
        "defaultListeningIP": "0.0.0.0",
        "caName": "ca",
        "externalName": "external",
        "ingressName": "ingress",
        "startAPI": "/start"
    },
    "envs": {
        "selfKeyTarget": "/secrets/self.key",
        "selfCertTarget": "/secrets/self.crt",
        "caCertTarget": "/secrets/ca.crt",
        "tlsTarget": "/secrets",
        "throttleInterval": 5,
        "upperBound": 4000000000
    },
    "fs": {
        "tlsFolder": "tls",
        "outputFolder": "output",
        "certExt": "crt",
        "keyExt": "key",
        "imageVersionFp": "fibonacci_image/latest_image.adoc"
    },
    "kube": {
        "helm": {
            "allowedKubeVersion": ">= 1.30.0",
            "apiVersion": "v2",
            "chartType": "application",
            "home": "https://github.com/laoluadewoye/Run-Fibonacci"
        },
        "podRestartPolicy": "Always",
        "replicas": 3
    },
    "platform": {
        "network": {
            "name": "run-fib-network",
            "driver": "bridge",
            "subnet": "172.19.0.0/16",
            "range": "172.19.0.0/24",
            "gateway": "172.19.0.1",
            "prefix": "172.19.0",
            "startAddress": 10
        },
        "startPort": 8080,
        "healthcheckCMD": "/usr/src/app/send_healthcheck.sh",
        "containerRestartPolicy": "on-failure",
        "containerFailPolicy": "kill"
    },
    "stage": {
        "composeNameSuffix": "app",
        "count": 4,
        "namePrefix": "server-stage",
        "startDelay": 5,
        "useCasePrefix": "use-case"
    },
    "tls": {
        "cert": {
            "validDaysCA": 2,
            "validDaysLeaf": 1
        },
        "rsa": {
            "keyLength": 4096,
            "publicExponent": 65537
        }
    }
}
