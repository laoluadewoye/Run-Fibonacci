{
    "dns": {
        "countryInitials": "US",
        "domain": "fibonaccitest.com",
        "datastoreName": "datastore",
        "default": "localhost",
        "defaultIP": "127.0.0.1",
        "defaultListeningIP": "0.0.0.0",
        "caName": "ca",
        "externalName": "external",
        "ingressName": "ingress",
        "startAPI": "/start"
    },
    "engine": {
        "network": {
            "name": "run-fib-network",
            "driver": "bridge",
            "subnet": "172.19.0.0/16",
            "range": "172.19.0.0/24",
            "gateway": "172.19.0.1",
            "prefix": "172.19.0",
            "startAddress": 20
        },
        "startPort": 8080,
        "healthcheckCMD": "/usr/src/app/send_healthcheck.py",
        "containerRestartPolicy": "on-failure",
        "containerFailPolicy": "kill"
    },
    "envs": {
        "selfName": "self",
        "tlsTarget": "/secrets",
        "throttleInterval": 5,
        "upperBound": 4000000000
    },
    "fs": {
        "tlsFolder": "tls",
        "outputFolder": "output",
        "certExt": "crt",
        "keyExt": "key",
        "pemExt": "pem",
        "imageVersionFp": "fibonacci_image/latest_image.adoc",
        "composeOutput": "docker-compose.json"
    },
    "orchestrator": {
        "helm": {
            "allowedKubeVersion": ">= 1.30.0",
            "apiVersion": "v2",
            "chartType": "application",
            "home": "https://github.com/laoluadewoye/Run-Fibonacci",
            "hook": "pre-install",
            "hookPolicy": "hook-failed",
            "templateFolder": "templates"
        },
        "namespacePolicy": "baseline",
        "podRestartPolicy": "Always",
        "replicas": 3
    },
    "server": {
        "api": "rest",
        "datastore": {
            "type": "none",
            "networkAddress": "172.19.0.11",
            "networkPort": 9000,
            "configs": {
                "elasticstack": {
                    "logstashImage": "docker.elastic.co/logstash/logstash:9.2.0",
                    "elasticsearchImage": "docker.elastic.co/elasticsearch/elasticsearch:9.2.0",
                    "kibanaImage": "docker.elastic.co/kibana/kibana:9.2.0"
                },
                "file": {
                    "format": "csv",
                    "logFolder": "/tmp",
                    "datastoreName": "datastore",
                    "defaultName": "default",
                    "operationsName": "operations"
                },
                "mongodb": {
                    "mongoImage": "docker.io/library/mongo:8.0.15-noble",
                    "mongoExpressImage": "docker.io/library/mongo-express:1.0.2-20-alpine3.19"
                },
                "postgresql": {
                    "postgresImage": "docker.io/library/postgres:18.0-alpine3.22",
                    "pgadminImage": "docker.io/elestio/pgadmin:REL-9_8"
                }
            }
        }
    },
    "stage": {
        "composeNameSuffix": "app",
        "count": 4,
        "namePrefix": "server-stage",
        "startDelay": 5,
        "useCasePrefix": "use-case"
    },
    "tls": {
        "cert": {
            "validDaysCA": 2,
            "validDaysLeaf": 1
        },
        "rsa": {
            "keyLength": 4096,
            "publicExponent": 65537
        }
    }
}
