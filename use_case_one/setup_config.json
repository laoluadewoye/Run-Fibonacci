{
    "dns": {
        "countryInitials": "US",
        "domain": "fibonaccitest.com",
        "default": "localhost",
        "caName": "ca",
        "externalName": "external"
    },
    "envs": {
        "selfKeyTarget": "/run/secrets/self.key",
        "selfCertTarget": "/run/secrets/self.crt",
        "caCertTarget": "/run/secrets/ca.crt",
        "throttleInterval": 5,
        "upperBound": 4000000000
    },
    "fs": {
        "outputFolder": "output",
        "certExt": "crt",
        "keyExt": "key"
    },
    "platform": {
        "network": {
            "name": "run-fib-one",
            "driver": "bridge",
            "subnet": "172.19.0.0/16",
            "range": "172.19.0.0/24",
            "gateway": "172.19.0.1",
            "prefix": "172.19.0",
            "startAddress": 10
        },
        "port": 8080,
        "restartPolicy": "unless-stopped"
    },
    "stage": {
        "composeNameSuffix": "app",
        "count": 3,
        "namePrefix": "server-stage",
        "startDelay": 5,
        "useCasePrefix": "use-case-1"
    },
    "tls": {
        "cert": {
            "validDaysCA": 2,
            "validDaysLeaf": 1
        },
        "rsa": {
            "keyLength": 4096,
            "publicExponent": 65537
        }
    }
}
